{
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  },
  "env" : {
    "browser" : true,
    "node" : true,
    "es6" : true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}